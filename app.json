{
  "name": "DARK_NERO",
  "description": "A WhatsApp MD bot built in Node.js to enhance the experience.",
  "logo": "https://i.ibb.co/zrvByTb/IMG-20230609-WA0083.jpg",
  "keywords": [
    "nodejs",
    "bot",
    "whatsapp bot",
    "dark-nero",
    "huttto",
    "cyber_yakuza",
    "nimesh",
    "pahan",
    "multi device",
    "baileys",
    "kaveesha"
  ],
  "success_url": "/",
  "stack": "container",
  "env": {
    "READ_MESSAGE": {
      "description": "Red Message on = true, off = false"
    },
    "ANTI_BAD": {
      "description": "Anti-ban on = true, off = false"
    },
    "ONLY_GROUP": {
      "description": "Restrict bot to groups only (on = true, off = false)"
    },
    "ANTI_LINK": {
      "description": "Anti-link protection (on = true, off = false)"
    },
    "ALIVE": {
      "description": "Add your custom alive message"
    },
    "OWENR_NO": {
      "description": "The owner's phone number",
      "value": "94786802371"
    },
    "LOGO": {
      "description": "Add a custom alive logo URL"
    },
    "HEOKU_API_KEY": {
      "description": "Heroku API Key"
    },
    "HEOKU_APP_NAME": {
      "description": "Heroku App Name"
    },
    "AOTO_VOICS": {
      "description": "Auto voice feature (on = true, off = false)"
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
